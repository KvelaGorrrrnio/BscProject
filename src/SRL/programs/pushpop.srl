// Testing the stack implementation: push and pop
// Input the stack size and then input the contents

// type declarations
int n int a
stack int l
stack int l'

// printing the list
procedure printl:
  from n = 0 do
    pop a l ! a
    n += 1
  until empty l
end

// the main procedure
procedure main: ? n
  from empty l do
    ? a push a l
    n -= 1
  until n = 0
  call printl ! n
end
